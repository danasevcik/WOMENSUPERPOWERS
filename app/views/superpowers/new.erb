<% if flash[:errors] %>
  <% flash[:errors].each do |error| %>
    <%= error %>
  <% end %>
<% end %>


<h1>ADD SUPERPOWER ðŸ‘‘</h1>
<%= form_for @superpower do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :New_Superpower? %>
  <%= f.select :seen_before?, [true,false] %>
  <br>
  <%= f.label :Which_Woman_Does_This_Belong_To? %>
  <%= f.collection_select(:woman_id, Woman.all, :id, :name) %>
